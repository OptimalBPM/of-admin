{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "declaration": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@angular/core": [
        "jspm_packages/npm/@angular/core@2.0.0-rc.1"
      ],
      "@angular/common": [
        "jspm_packages/npm/@angular/common@2.0.0-rc.1"
      ],
      "@angular/compiler": [
        "jspm_packages/npm/@angular/compiler@2.0.0-rc.1"
      ],
      "@angular/router": [
        "jspm_packages/npm/@angular/router@2.0.0-rc.1"
      ],
      "@angular/platform-browser-dynamic": [
        "jspm_packages/npm/@angular/platform-browser-dynamic@2.0.0-rc.1"
      ],
      "rxjs/*": [
        "jspm_packages/npm/rxjs@5.0.0-beta.6/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "jspm_packages",
    "build",
    "dist",
    "typings/index.d.ts",
    "typings/modules"
  ],
  "compileOnSave": false
}